{
  "name": "sorted-list-client",
  "version": "2.0.0",
  "description": "Simple data view component. (react, redux, es6, webpack, jest)",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "start:unsafe": "NODE_ENV=unsafe node index.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --env production",
    "build:server": "webpack --config webpack.server.js",
    "dev": "npm run dev:server && npm run dev:start && npm run dev:client",
    "dev:start": "nodemon index.js",
    "dev:client": "webpack-dev-server --env development --color",
    "dev:server": "webpack --config webpack.server.js --watch",
    "mock": "NODE_ENV=test node . && webpack-dev-server --env development --color",
    "mock:server": "NODE_ENV=test node .",
    "test": "jest --coverage",
    "lint": "eslint ./src",
    "format": "prettier \"**/*.+(js|jsx|json|css)\" --write",
    "validate-format": "prettier \"**/*.+(js|jsx|json|css)\" --list-different"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BriptimusPrimus/sorted-list-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BriptimusPrimus/sorted-list-client/issues"
  },
  "homepage": "https://github.com/BriptimusPrimus/sorted-list-client#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.4.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^3.0.0",
    "morgan": "^1.7.0",
    "mysql": "^2.18.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/react": "^11.0.4",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.3",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "mini-css-extract-plugin": "^0.11.0",
    "nodemon": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "supertest": "^2.0.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.3",
    "webpack-node-externals": "^2.5.2"
  }
}
